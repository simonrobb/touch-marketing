{
  "name": "marketing",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "better-npm-run dev",
    "start": "better-npm-run start:prod",
    "build": "better-npm-run build"
  },
  "betterScripts": {
    "dev": {
      "command": "webpack-dev-server --config webpack/webpack.config.js --port 9000 --history-api-fallback --color --progress",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "start:prod": {
      "command": "npm run build && node server/index.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build": {
      "command": "rm -rf build/ && webpack --config webpack/webpack.config.js --color --progress",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "babel": {
    "presets": [
      "es2015",
      "es2016",
      "react",
      "stage-0",
      "stage-1"
    ],
    "plugins": [
      "react-hot-loader/babel"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/touch-tech/marketing.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/touch-tech/marketing/issues"
  },
  "homepage": "https://github.com/touch-tech/marketing#readme",
  "dependencies": {
    "autoprefixer": "6.3.7",
    "babel-polyfill": "^6.9.1",
    "better-npm-run": "0.0.11",
    "chalk": "^1.1.3",
    "classnames": "^2.2.5",
    "express": "^4.14.0",
    "html-webpack-plugin": "^2.22.0",
    "lost": "^7.1.0",
    "postcss-color-function": "^2.0.1",
    "postcss-mixins": "^5.4.0",
    "postcss-nested": "^1.0.0",
    "postcss-simple-vars": "^3.0.0",
    "webpack": "^1.13.2"
  },
  "devDependencies": {
    "axios": "^0.14.0",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-es2016": "6.11.3",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "css-loader": "^0.24.0",
    "file-loader": "^0.9.0",
    "jsdom": "^9.5.0",
    "mocha": "^3.0.2",
    "moment": "^2.15.1",
    "plottable": "^2.2.0",
    "postcss-loader": "^0.11.1",
    "react": "^15.3.2",
    "react-chartjs-2": "^1.2.6",
    "react-datepicker": "^0.29.0",
    "react-dom": "^15.3.0",
    "react-hot-loader": "^3.0.0-beta.2",
    "react-router": "^2.8.1",
    "react-sparklines": "^1.6.0",
    "style-loader": "^0.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.12.1"
  }
}
